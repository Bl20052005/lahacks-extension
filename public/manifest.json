{
  "name": "EcoEval",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_title": "EcoEval",
    "default_icon": "images/plant.png"
  },
  "permissions": [
    "scripting",
    "tabs",
    "storage",
    "history",
    "bookmarks",
    "sidePanel"
  ],
  "icons": {
    "16": "images/plant.png",
    "32": "images/plant.png",
    "48": "images/plant.png",
    "128": "images/plant.png"
  },
  "host_permissions": ["http://*/*", "https://*/*"],
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
